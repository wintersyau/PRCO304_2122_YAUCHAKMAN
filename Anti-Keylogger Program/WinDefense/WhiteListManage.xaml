<Window x:Class="WinDefense.WhiteListManage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinDefense"
        mc:Ignorable="d"
        Title="WhiteListManage" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="25"/>
            </Grid.RowDefinitions>
            
            <ListBox Name="Whites" Grid.Row="0">

            </ListBox>

            <Button Click="AnyBtnDown" Grid.Row="1">
                Remove
            </Button>
        </Grid>
       
    </Grid>
</Window>
